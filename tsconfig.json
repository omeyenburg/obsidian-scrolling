{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": [
      "DOM",
      "ES2021"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "removeComments": true,
    "skipLibCheck": true,
    "target": "ES2021"
  },
  "include": [
    "src"
  ]
}
